{
    "name": "Better Google Search",
    "description": "Better google search results UI",
    "version": "0.1",
    "permissions": [
    "tabs","<all_urls>"
    ],

    "content_scripts": [
        {
        "matches": [
            "http://*.google.com/*",
            "https://*.google.com/*"
            ],
        "css": ["fix.css"],
        "run_at": "document_end"         // pay attention to this line
        }
    ], 
    "manifest_version":2
}